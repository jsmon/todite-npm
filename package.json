{
  "name": "todite",
  "version": "0.1.1",
  "description": "Connect your application to Todite, a free to-do list app",
  "main": "dist/index.min.js",
  "browser": "dist/browser.min.js",
  "bin": "dist/cli.min.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc && terser dist/index.js -c -m --toplevel -o dist/index.min.js --source-map \"url='index.min.js.map', content='./dist/index.js.map'\" && terser dist/browser.js -c -m -o dist/browser.min.js --source-map \"url='browser.min.js.map', content='./dist/browser.js.map'\" && terser dist/cli.js -c -m --toplevel -o dist/cli.min.js --source-map \"url='cli.min.js.map', content='./dist/cli.js.map'\"",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "lint": "eslint \"src/**\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsmon/todite-npm.git"
  },
  "homepage": "https://todite.vercel.app/docs/npm/v0.1",
  "keywords": [
    "todite",
    "to-do",
    "todo",
    "todolist",
    "to-dolist",
    "to-do-list",
    "cli",
    "command-line-interface",
    "front-end",
    "frontend",
    "back-end",
    "backend"
  ],
  "author": {
    "name": "James Simon",
    "email": "jsmon.dev@gmail.com",
    "url": "https://jsmon.dev/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsmon/todite-npm/issues",
    "email": "jsmon.dev@gmail.com"
  },
  "dependencies": {
    "commander": "^7.2.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "terser": "^5.6.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
