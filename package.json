{
  "name": "todite",
  "version": "0.1.0",
  "description": "Connect your application to Todite, a free to-do list app",
  "main": "dist/index.min.js",
  "browser": "dist/browser.min.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc && terser dist/index.js -c -m --toplevel -o dist/index.min.js --source-map \"url='index.min.js.map', content='./dist/index.js.map'\" && terser dist/browser.js -c -m -o dist/browser.min.js --source-map \"url='browser.min.js.map', content='./dist/browser.js.map'\"",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsmon/todite-npm.git"
  },
  "homepage": "https://todite.now.sh/docs/npm/v1.0",
  "keywords": [
    "todite",
    "to-do",
    "todo",
    "todolist"
  ],
  "author": {
    "name": "James Simon",
    "email": "jsmon.dev@gmail.com",
    "url": "https://jsmon.vercel.app/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsmon/todite-npm/issues",
    "email": "jsmon.dev@gmail.com"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.5.8",
    "jest": "^26.6.3",
    "terser": "^5.6.0",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
